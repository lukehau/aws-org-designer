{
  "name": "aws-org-designer",
  "productName": "AWS Org Designer",
  "version": "1.0.3",
  "description": "Visual designer and management tool for AWS Organizations",
  "author": "Luke Harris",
  "private": true,
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "electron:dev": "electron-vite dev",
    "electron:build:mac": "electron-vite build && electron-builder --mac --publish never",
    "electron:build:win": "electron-vite build && electron-builder --win --publish never",
    "electron:release:mac": "electron-vite build && electron-builder --mac --publish onTag",
    "electron:release:win": "electron-vite build && electron-builder --win --publish onTag"
  },
  "dependencies": {
