{
  "name": "aws-org-designer",
  "productName": "AWS Org Designer",
  "version": "1.0.0",
  "description": "Visual designer and management tool for AWS Organizations",
  "author": "Luke Hau",
  "private": true,
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "electron:dev": "electron-vite dev",
    "electron:build": "electron-vite build",
    "electron:preview": "electron-vite preview",
    "electron:pack": "electron-vite build && electron-builder --dir",
    "electron:release": "electron-vite build && electron-builder --publish always",
    "electron:release:mac": "electron-vite build && electron-builder --mac --publish always",
    "electron:release:win": "electron-vite build && electron-builder --win --publish always",
    "cdk:install": "cd cdk && npm install",
    "cdk:synth:dev": "cd cdk && npm run synth:dev",
    "cdk:synth:prod": "cd cdk && npm run synth:prod",
    "cdk:deploy:dev": "cd cdk && npm run deploy:dev",
    "cdk:deploy:prod": "cd cdk && npm run deploy:prod"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.2",
